{
  "manifest_version": 2,
  "name": "gpu",
  "description": "Get gpu values bench",
  "version": "1.0.0",
  "icons": {
    "256": "icon_256.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["https://*.placasdevideo.app.br/*", "https://placasdevideo.app.br/*"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}
